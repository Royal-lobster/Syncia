#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/srujangurram/Projects/personal/syncia-plasmo/node_modules/.pnpm/lmdb@2.7.11/node_modules/lmdb/bin/node_modules:/Users/srujangurram/Projects/personal/syncia-plasmo/node_modules/.pnpm/lmdb@2.7.11/node_modules/lmdb/node_modules:/Users/srujangurram/Projects/personal/syncia-plasmo/node_modules/.pnpm/lmdb@2.7.11/node_modules:/Users/srujangurram/Projects/personal/syncia-plasmo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/srujangurram/Projects/personal/syncia-plasmo/node_modules/.pnpm/lmdb@2.7.11/node_modules/lmdb/bin/node_modules:/Users/srujangurram/Projects/personal/syncia-plasmo/node_modules/.pnpm/lmdb@2.7.11/node_modules/lmdb/node_modules:/Users/srujangurram/Projects/personal/syncia-plasmo/node_modules/.pnpm/lmdb@2.7.11/node_modules:/Users/srujangurram/Projects/personal/syncia-plasmo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../lmdb@2.7.11/node_modules/lmdb/bin/download-prebuilds.js" "$@"
else
  exec node  "$basedir/../../../../../../lmdb@2.7.11/node_modules/lmdb/bin/download-prebuilds.js" "$@"
fi
